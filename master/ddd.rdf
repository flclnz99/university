        <rdfs:domain rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#PS"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
        <rdfs:comment xml:lang="it">Riferimento a una specifica console PlayStation, come PlayStation 4 o PlayStation 5. Indica la piattaforma su cui un videogioco è disponibile o ottimizzato, offrendo miglioramenti nelle prestazioni, grafica e funzionalità specifiche per l&apos;hardware della console PlayStation.</rdfs:comment>
        <rdfs:label xml:lang="it">versionePS</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#versioneXBOX -->

    <owl:DatatypeProperty rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#versioneXBOX">
        <rdfs:domain rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#XBOX"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="it">Riferimento a una specifica console Xbox, come Xbox One o Xbox Series X/S. Indica la piattaforma su cui un videogioco è disponibile o ottimizzato per offrire una migliore esperienza di gioco, in termini di grafica, prestazioni e funzionalità.</rdfs:comment>
        <rdfs:label xml:lang="it">versioneXBOX</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/net/VideoGameOntology#vgo:Game -->

    <owl:Class rdf:about="http://purl.org/net/VideoGameOntology#vgo:Game">
        <owl:equivalentClass rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Videogioco"/>
        <rdfs:comment xml:lang="it">vgo:Game rappresenta un videogioco nell&apos;ontologia VGO (Video Game Ontology).</rdfs:comment>
        <rdfs:label xml:lang="it">vgo:Game</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Item -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Item"/>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/list.owl#ListItem -->

    <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/cp/owl/list.owl#ListItem">
        <rdfs:comment xml:lang="it">Insieme delle parti fondamentali di un videogioco. Contiene: DLC, genere, oggetto, arma</rdfs:comment>
    </rdf:Description>
    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Antagonista -->

    <owl:Class rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Antagonista">
        <rdfs:subClassOf rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Personaggio"/>
        <rdfs:comment xml:lang="it">Antagonista rappresenta il personaggio opposto al protagonista, spesso incaricato di ostacolare la sua missione o obiettivi, contribuendo al conflitto narrativo del videogioco.</rdfs:comment>
        <rdfs:label xml:lang="it">Antagonista</rdfs:label>
    </owl:Class>
    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Arma -->

    <owl:Class rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Arma">
        <rdfs:subClassOf rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Oggetto"/>
        <rdfs:comment xml:lang="it">Arma rappresenta un tipo di oggetto utilizzato nei videogiochi per attaccare nemici o difendersi. Può includere armi da mischia, da fuoco, magiche o futuristiche, a seconda del contesto di gioco.</rdfs:comment>
        <rdfs:label xml:lang="it">Arma</rdfs:label>
    </owl:Class>
    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#DLC -->

    <owl:Class rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#DLC">
        <rdfs:subClassOf rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Elemento_di_Gioco"/>
        <rdfs:comment xml:lang="it">DLC (Downloadable Content) rappresenta un contenuto aggiuntivo scaricabile per un videogioco, che può includere nuove missioni, personaggi, oggetti o espansioni, estendendo l&apos;esperienza di gioco.</rdfs:comment>
        <rdfs:label xml:lang="it">DLC</rdfs:label>
    </owl:Class>
    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Elemento_di_Gioco -->

    <owl:Class rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Elemento_di_Gioco">
        <rdfs:comment xml:lang="it">Elemento di gioco rappresenta qualsiasi componente interno a un videogioco, inclusi eventi, oggetti, DLC, generi e aspetti della produzione. Serve a categorizzare elementi distintivi che caratterizzano l’esperienza di gioco.</rdfs:comment>
        <rdfs:label xml:lang="it">Elemento di Gioco</rdfs:label>
    </owl:Class>
    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Evento_di_Gioco -->

    <owl:Class rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Evento_di_Gioco">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/list.owl#ListItem"/>
        <rdfs:comment xml:lang="it">Evento di gioco rappresenta un&apos;azione o situazione rilevante che si verifica durante il gameplay, come missioni, livelli, boss fight o eventi speciali, influenzando la progressione del videogioco.</rdfs:comment>
        <rdfs:label xml:lang="it">Evento di Gioco</rdfs:label>
    </owl:Class>
    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Genere -->

    <owl:Class rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Genere">
        <owl:equivalentClass rdf:resource="http://xmlns.com/foaf/0.1/Genre"/>
        <rdfs:subClassOf rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Elemento_di_Gioco"/>
        <rdfs:comment xml:lang="it">Genere rappresenta la categoria di appartenenza di un videogioco in base alle sue meccaniche di gioco, narrativa e stile, come azione, RPG o strategia. Può avere sottogeneri per una classificazione più specifica.</rdfs:comment>
        <rdfs:label xml:lang="it">Genere</rdfs:label>
    </owl:Class>
    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Nintendo -->

    <owl:Class rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Nintendo">
        <rdfs:subClassOf rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Piattaforma"/>
        <rdfs:comment xml:lang="it">Azienda giapponese leader nell&apos;industria dei videogiochi, conosciuta per le sue console come la Nintendo Switch, Nintendo 3DS e Wii.</rdfs:comment>
        <rdfs:label xml:lang="it">Nintendo</rdfs:label>
    </owl:Class>
    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Oggetto -->

    <owl:Class rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Oggetto">
        <rdfs:subClassOf rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Elemento_di_Gioco"/>
        <rdfs:comment xml:lang="it">Oggetto rappresenta qualsiasi elemento interattivo all&apos;interno di un videogioco, come armi, equipaggiamenti o oggetti collezionabili, che possono influenzare il gameplay o l&apos;esperienza del giocatore.</rdfs:comment>
        <rdfs:label xml:lang="it">Oggetto</rdfs:label>
    </owl:Class>
    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#PC -->

    <owl:Class rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#PC">
        <rdfs:subClassOf rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Piattaforma"/>
        <rdfs:comment xml:lang="it">PC rappresenta una piattaforma di gioco basata su personal computer, caratterizzata da hardware configurabile, compatibilità con periferiche diverse e distribuzione digitale dei videogiochi.</rdfs:comment>
        <rdfs:label xml:lang="it">PC</rdfs:label>
    </owl:Class>
    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#PEGI -->

    <owl:Class rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#PEGI">
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#PEGI12"/>
                    <rdf:Description rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#PEGI16"/>
                    <rdf:Description rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#PEGI18"/>
                    <rdf:Description rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#PEGI3"/>
                    <rdf:Description rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#PEGI7"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment xml:lang="it">PEGI rappresenta il sistema di classificazione per l’età consigliata di un videogioco, basato su contenuti come violenza, linguaggio e temi sensibili. È una classe enumerata con valori predefiniti (3, 7, 12, 16, 18).</rdfs:comment>
        <rdfs:label xml:lang="it">PEGI</rdfs:label>
    </owl:Class>
    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#PS -->

    <owl:Class rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#PS">
        <rdfs:subClassOf rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Piattaforma"/>
        <rdfs:comment xml:lang="it">PS (PlayStation) è una piattaforma di gioco sviluppata da Sony, composta da console dedicate, con un ampio catalogo di videogiochi, supporto per la realtà virtuale e funzionalità online.</rdfs:comment>
        <rdfs:label xml:lang="it">PS</rdfs:label>
    </owl:Class>
    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Personaggio -->

    <owl:Class rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Personaggio">
        <rdfs:comment xml:lang="it">Personaggio rappresenta un&apos;entità presente nel videogioco, controllabile o meno, che può svolgere ruoli chiave nella trama o nel gameplay, come protagonista o antagonista.</rdfs:comment>
        <rdfs:label xml:lang="it">Personaggio</rdfs:label>
    </owl:Class>
    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Piattaforma -->

    <owl:Class rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Piattaforma">
        <rdfs:comment xml:lang="it">Piattaforma rappresenta il sistema hardware o software su cui un videogioco può essere eseguito, includendo console, PC e dispositivi mobili.</rdfs:comment>
        <rdfs:label xml:lang="it">Piattaforma</rdfs:label>
    </owl:Class>
    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Produzione -->

    <owl:Class rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Produzione">
        <rdfs:comment xml:lang="it">Produzione rappresenta il processo di creazione e distribuzione di un videogioco, includendo attività come lo sviluppo, la progettazione, il marketing e la pubblicazione, con il coinvolgimento di vari attori, tra cui publisher e sviluppatori.</rdfs:comment>
        <rdfs:label xml:lang="it">Produzione</rdfs:label>
    </owl:Class>
    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Protagonista -->

    <owl:Class rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Protagonista">
        <owl:equivalentClass rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#vgo:Character"/>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#presenteIn"/>
                <owl:someValuesFrom rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Videogioco"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Personaggio"/>
        <rdfs:comment xml:lang="it">Protagonista rappresenta il personaggio principale di un videogioco, spesso controllato dal giocatore, attorno al quale si sviluppa la trama e le principali dinamiche di gameplay.</rdfs:comment>
        <rdfs:label xml:lang="it">Protagonista</rdfs:label>
    </owl:Class>
    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Publisher -->

    <owl:Class rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Publisher">
        <rdfs:subClassOf rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Produzione"/>
        <rdfs:comment xml:lang="it">Publisher è l&apos;entità responsabile della distribuzione e commercializzazione di un videogioco, gestendo aspetti come la promozione, la vendita e la pubblicazione sui vari mercati, spesso finanziando e supervisionando lo sviluppo del gioco.</rdfs:comment>
        <rdfs:label xml:lang="it">Publisher</rdfs:label>
    </owl:Class>
    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Sviluppatore -->

    <owl:Class rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Sviluppatore">
        <rdfs:subClassOf rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Produzione"/>
        <rdfs:comment xml:lang="it">Sviluppatore è l&apos;entità o il team responsabile della creazione e dello sviluppo tecnico di un videogioco, occupandosi della progettazione, della programmazione, della grafica e della realizzazione di tutti gli aspetti interattivi del gioco.</rdfs:comment>
        <rdfs:label xml:lang="it">Sviluppatore</rdfs:label>
    </owl:Class>
    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Videogioco -->

    <owl:Class rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Videogioco">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#disponibileSu"/>
                <owl:someValuesFrom rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Piattaforma"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#haPublisher"/>
                <owl:someValuesFrom rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Publisher"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#sviluppatoDa"/>
                <owl:someValuesFrom rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Sviluppatore"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#haEvento"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Evento_di_Gioco"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/list.owl#List"/>
        <rdfs:comment xml:lang="it">Videogioco rappresenta un prodotto software interattivo progettato per l’intrattenimento, giocabile su diverse piattaforme e caratterizzato da elementi come genere, modalità di gioco, storia e personaggi.</rdfs:comment>
        <rdfs:label xml:lang="it">Videogioco</rdfs:label>
    </owl:Class>
    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#XBOX -->

    <owl:Class rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#XBOX">
        <rdfs:subClassOf rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Piattaforma"/>
        <rdfs:comment xml:lang="it">Xbox è una piattaforma di gioco sviluppata da Microsoft, composta da console dedicate con supporto per giochi ad alta definizione, giochi online tramite Xbox Live e servizi di abbonamento come Xbox Game Pass.</rdfs:comment>
        <rdfs:label xml:lang="it">XBOX</rdfs:label>
    </owl:Class>
    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#vgo:Character -->

    <owl:Class rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#vgo:Character">
        <rdfs:subClassOf rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Personaggio"/>
        <rdfs:label xml:lang="it">vgo:Character</rdfs:label>
        <rdfs:label xml:lang="it">vgo:Character rappresenta un personaggio all&apos;interno dell&apos;ontologia VGO (Video Game Ontology).</rdfs:label>
    </owl:Class>
    


    <!-- http://xmlns.com/foaf/0.1/Genre -->

    <owl:Class rdf:about="http://xmlns.com/foaf/0.1/Genre">
        <rdfs:comment xml:lang="it">Rappresenta un genere associato a un&apos;entità, spesso utilizzato per categorizzare opere creative come film, libri o videogiochi.</rdfs:comment>
        <rdfs:label xml:lang="it">foaf:Genre</rdfs:label>
    </owl:Class>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Ascia_da_Guerra -->

    <owl:NamedIndividual rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Ascia_da_Guerra">
        <rdfs:comment xml:lang="it">Ascia da guerra utilizzata come arma in combattimento.</rdfs:comment>
        <rdfs:label xml:lang="it">AsciaDaGuerra</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Ash -->

    <owl:NamedIndividual rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Ash">
        <videogame:usaArma rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Pokeball"/>
        <rdfs:comment xml:lang="it">Ash</rdfs:comment>
        <rdfs:comment xml:lang="it">Protagonista della serie animata e dei videogiochi Pokémon, ispirato al personaggio Red dei videogiochi. È un Allenatore Pokémon determinato a diventare un Maestro Pokémon, accompagnato dal suo fedele Pikachu.</rdfs:comment>
    </owl:NamedIndividual>
    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Azione -->

    <owl:NamedIndividual rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Azione">
        <videogame:haSottogenere rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Gioco_di_Ruolo"/>
        <rdfs:comment xml:lang="it">Azione è un tipo di videogioco in cui l&apos;enfasi è posta su sfide fisiche, combattimenti e azioni rapide. I giochi di azione includono sequenze di gioco che richiedono riflessi rapidi, coordinazione e abilità nel superare ostacoli o nemici. Gli esempi classici sono i giochi di platform, sparatutto e giochi di combattimento.</rdfs:comment>
        <rdfs:label xml:lang="it">Azione</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Baldur -->

    <owl:NamedIndividual rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Baldur">
        <rdfs:comment xml:lang="it">Antagonista principale del videogioco God Of War</rdfs:comment>
        <rdfs:label xml:lang="it">Baldur</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Gioco_di_Ruolo -->

    <owl:NamedIndividual rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Gioco_di_Ruolo">
        <rdfs:comment xml:lang="it">Genere di videogiochi in cui i giocatori controllano uno o più personaggi, sviluppandone abilità e statistiche attraverso missioni, combattimenti e interazioni con il mondo di gioco.</rdfs:comment>
        <rdfs:label xml:lang="it">GiocoDiRuolo</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#God_Of_War_Ragnarok -->

    <owl:NamedIndividual rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#God_Of_War_Ragnarok">
        <videogame:contienePersonaggio rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Kratos"/>
        <videogame:disponibileSu rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Intel_Core_i3-12100"/>
        <videogame:disponibileSu rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#PS4"/>
        <videogame:haAntagonista rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Baldur"/>
        <videogame:haDLC rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Valhalla"/>
        <videogame:haGenere rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Azione"/>
        <videogame:haPEGI rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#PEGI18"/>
        <videogame:haPublisher rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Sony"/>
        <videogame:sviluppatoDa rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Santa_Monica_Studios"/>
        <videogame:haAnnoDiUscita rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2019</videogame:haAnnoDiUscita>
        <videogame:numeroGiocatori rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</videogame:numeroGiocatori>
        <rdfs:comment xml:lang="it">God of War è un videogioco di azione-avventura, noto per la sua narrativa coinvolgente e il combattimento dinamico. Il gioco segue le avventure di Kratos, un ex dio greco, e suo figlio Atreus, mentre affrontano sfide mitologiche in un mondo ispirato alla mitologia norrena. God Of War Ragnarok è l&apos;ultimo capitolo della saga principale</rdfs:comment>
        <rdfs:label xml:lang="it">GodOfWarRagnarok</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Intel_Core_i3-12100 -->

    <owl:NamedIndividual rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Intel_Core_i3-12100">
        <videogame:versionePC>Intel Core i3-12100</videogame:versionePC>
        <rdfs:comment xml:lang="it">Intel Core i3-12100: PC basato su processore Intel Core i3-12100, appartenente alla dodicesima generazione di CPU Intel. Ideale per gaming entry-level e attività generali, compatibile con sistemi Windows e Linux.</rdfs:comment>
        <rdfs:label xml:lang="it">IntelCorei3-12100</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Kratos -->

    <owl:NamedIndividual rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Kratos">
        <videogame:usaArma rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Ascia_da_Guerra"/>
        <rdfs:comment xml:lang="it">Kratos è il protagonista della serie di videogiochi *God of War*. Ex dio della guerra nella mitologia greca, è caratterizzato da un temperamento impulsivo e una forza straordinaria. Nel corso della serie, Kratos affronta numerosi nemici e divinità, lottando con il suo passato tormentato mentre cerca di proteggere suo figlio, Atreus, e affrontare nuove sfide mitologiche.</rdfs:comment>
        <rdfs:label xml:lang="it">Kratos</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Midgard -->

    <owl:NamedIndividual rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Midgard">
        <list:firstItemOf rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#God_Of_War_Ragnarok"/>
        <videogame:tipoEvento>Livello</videogame:tipoEvento>
        <rdfs:comment xml:lang="it">Livello iniziale del videogioco God of War: Ragnarok</rdfs:comment>
        <rdfs:label xml:lang="it">Midgard</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Nintendo_Switch -->

    <owl:NamedIndividual rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Nintendo_Switch">
        <videogame:versioneNintendo>Switch</videogame:versioneNintendo>
        <rdfs:comment xml:lang="it">Console ibrida prodotta da Nintendo, che può essere utilizzata sia come una console da casa collegata a una TV, sia come una console portatile grazie al suo design modulare.</rdfs:comment>
        <rdfs:label xml:lang="it">NintendoSwitch</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Odino -->

    <owl:NamedIndividual rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Odino">
        <list:lastItemOf rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#God_Of_War_Ragnarok"/>
        <sequence:follows rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Midgard"/>
        <videogame:tipoEvento>Boss</videogame:tipoEvento>
        <rdfs:comment xml:lang="it">Odino, padre degli dei norreni, è il boss finale con cui Kratos, protagonista del videogioco God of War: Ragnarok, si scontra.</rdfs:comment>
        <rdfs:label xml:lang="it">Odino</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#PEGI12 -->

    <owl:NamedIndividual rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#PEGI12">
        <rdfs:comment xml:lang="it">PEGI 12 è una classificazione di età fornita dal sistema di classificazione dei videogiochi PEGI (Pan European Game Information). Indica che un videogioco è adatto per bambini di 12 anni o più, e che potrebbe contenere elementi come violenza moderata, linguaggio inappropriato o contenuti che non sono adatti ai più giovani, ma che sono considerati accettabili per un pubblico più maturo.</rdfs:comment>
        <rdfs:label xml:lang="it">PEGI12</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#PEGI16 -->

    <owl:NamedIndividual rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#PEGI16">
        <rdfs:comment xml:lang="it">PEGI 16 è una classificazione di età del sistema PEGI, che indica che un videogioco è adatto per giocatori di 16 anni o più. I giochi con questa classificazione possono contenere violenza più intensa, linguaggio forte, contenuti sessuali o riferimenti, e tematiche mature che potrebbero non essere adatti a un pubblico più giovane.</rdfs:comment>
        <rdfs:label xml:lang="it">PEGI16</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#PEGI18 -->

    <owl:NamedIndividual rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#PEGI18">
        <rdfs:comment xml:lang="it">PEGI 18 è la classificazione più alta nel sistema PEGI, destinata ai videogiochi che sono adatti solo a un pubblico adulto, ovvero 18 anni e oltre. I giochi con questa classificazione possono contenere contenuti estremi, come violenza grafica, linguaggio volgare, tematiche sessuali esplicite, droghe, e altre situazioni o comportamenti non adatti ai minori.</rdfs:comment>
        <rdfs:label xml:lang="it">PEGI18</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#PEGI3 -->

    <owl:NamedIndividual rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#PEGI3">
        <rdf:type rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#PEGI"/>
        <rdfs:comment xml:lang="it">PEGI 3 è la classificazione più bassa nel sistema PEGI, indicata per videogiochi adatti a tutte le età, inclusi i bambini molto piccoli. I giochi con questa classificazione non contengono elementi di violenza, linguaggio inappropriato o altri contenuti che potrebbero essere dannosi per i più giovani. Di solito, questi giochi sono semplici, educativi o basati su elementi di gioco non violenti.</rdfs:comment>
        <rdfs:label xml:lang="it">PEGI3</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#PEGI7 -->

    <owl:NamedIndividual rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#PEGI7">
        <rdfs:comment xml:lang="it">PEGI 7 è una classificazione che indica che il videogioco è adatto per bambini a partire dai 7 anni. I giochi con questa classificazione possono contenere elementi di violenza molto leggera o situazioni spaventose, ma senza grafiche o contenuti eccessivamente realistici. In genere, i giochi PEGI 7 sono ancora adatti ai più giovani, con tematiche e contenuti che non superano il loro livello di comprensione o sensibilità.</rdfs:comment>
        <rdfs:label xml:lang="it">PEGI7</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#PS4 -->

    <owl:NamedIndividual rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#PS4">
        <videogame:versionePS rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</videogame:versionePS>
        <rdfs:comment xml:lang="it">PS4 (PlayStation 4) è una console di gioco sviluppata da Sony, lanciata nel 2013. Fa parte della quarta generazione di console PlayStation e offre una vasta gamma di giochi, supporto per grafica ad alta definizione, streaming multimediale, funzionalità online attraverso PlayStation Network e servizi come PlayStation Plus. La PS4 è conosciuta per la sua potenza hardware, il design compatto e l&apos;ampia libreria di giochi.</rdfs:comment>
        <rdfs:label xml:lang="it">PS4</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Pokeball -->

    <owl:NamedIndividual rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Pokeball">
        <rdfs:comment xml:lang="it">Oggetto iconico del franchise Pokémon utilizzato per catturare e immagazzinare Pokémon. Esistono diverse varianti con effetti specifici, influenzando la probabilità di cattura.</rdfs:comment>
        <rdfs:label xml:lang="it">Pokeball</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Pokemon_Scarlatto -->

    <owl:NamedIndividual rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Pokemon_Scarlatto">
        <videogame:contienePersonaggio rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Ash"/>
        <videogame:disponibileSu rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Nintendo_Switch"/>
        <videogame:haDLC rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#TesoroAreaZero"/>
        <videogame:haGenere rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Gioco_di_Ruolo"/>
        <videogame:haPEGI rdf:resource="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#PEGI3"/>
        <rdfs:comment xml:lang="it">Videogioco di ruolo sviluppato da Game Freak e pubblicato da Nintendo e The Pokémon Company. Appartiene al franchise Pokémon ed è esclusivo per Nintendo Switch.</rdfs:comment>
        <rdfs:label xml:lang="it">PokemonScarlatto</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Santa_Monica_Studios -->

    <owl:NamedIndividual rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Santa_Monica_Studios">
        <rdfs:comment xml:lang="it">Sviluppatore di videogiochi statunitense, parte dei PlayStation Studios di Sony. È noto principalmente per la serie God of War, caratterizzata da una forte componente narrativa e un gameplay d&apos;azione.</rdfs:comment>
        <rdfs:label xml:lang="it">SantaMonicaStudio</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Sony -->

    <owl:NamedIndividual rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Sony">
        <rdfs:comment xml:lang="it">Azienda giapponese specializzata in elettronica di consumo, gaming e intrattenimento. Publisher e produttore della console PlayStation, nonché sviluppatore di videogiochi tramite i suoi studi first-party.</rdfs:comment>
        <rdfs:label xml:lang="it">Sony</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#TesoroAreaZero -->

    <owl:NamedIndividual rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#TesoroAreaZero">
        <rdfs:comment xml:lang="it">Contenuto aggiuntivo (DLC) per *Pokémon Scarlatto* e *Pokémon Violetto*, sviluppato da Game Freak e pubblicato da Nintendo e The Pokémon Company. Espande la storia e introduce nuove aree, Pokémon e missioni.</rdfs:comment>
        <rdfs:label xml:lang="it">TesoroAreaZero</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Valhalla -->

    <owl:NamedIndividual rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#Valhalla">
        <rdfs:isDefinedBy xml:lang="it">Contenuto aggiuntivo per God of War (2018), che espande la trama principale e introduce nuove ambientazioni, missioni e nemici, ispirati alla mitologia norrena. Il DLC esplora aspetti legati alla mitologia di Valhalla, arricchendo l&apos;esperienza di gioco.</rdfs:isDefinedBy>
        <rdfs:label xml:lang="it">Valhalla</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#XBOX_ONE -->

    <owl:NamedIndividual rdf:about="http://www.videogame.org/lorenzo/ontologies/2024/1/videogame#XBOX_ONE">
        <videogame:versioneXBOX>ONE</videogame:versioneXBOX>
        <rdfs:comment xml:lang="it">Console prodotta da Microsoft, appartenente all’ottava generazione di console. Supporta giochi in formato digitale e fisico ed è parte dell’ecosistema Xbox.</rdfs:comment>
        <rdfs:label xml:lang="it">XBOXONE</rdfs:label>
    </owl:NamedIndividual>
    


